{
  "description": "UnGooglify sinkholes google.com domain traffic.",
  "manifest_version": 2,
  "name": "UnGooglify",
  "version": "1.1",
  
  "icons": {
    "32": "icons/ung-32.png"
	},

  "permissions": [
    "webRequest","webRequestBlocking",
	 "storage", "notifications",
    "*://*.google.com/*"
   ],

  "background": {
    "scripts": ["background.js"]
   },

  "browser_action": {
  	"default_icon": "icons/ung-32.png",
   "default_title": "UnGooglify"
  }


}

